<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

   <display-name>vega-approach-2</display-name>

   <welcome-file-list>
      <welcome-file>index.jsp</welcome-file>
   </welcome-file-list>
   
   <servlet>
      <servlet-name>CategoryController</servlet-name>
      <servlet-class>org.gmnz.vega.ui.CategoryController</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>CategoryController</servlet-name>
      <url-pattern>/category/*</url-pattern>
   </servlet-mapping>

   <servlet>
      <servlet-name>CategoryExecution</servlet-name>
      <servlet-class>org.gmnz.vega.ui.CategoryExecution</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>CategoryExecution</servlet-name>
      <url-pattern>/category/do/*</url-pattern>
   </servlet-mapping>



</web-app>
